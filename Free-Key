local Tab1 = Window:AddTab({ Title = "Combat", Icon = "swords" })
local Tab2 = Window:AddTab({ Title = "Spam", Icon = "rbxassetid://10709781460" })
local Tab3 = Window:AddTab({ Title = "Detection", Icon = "shield" })

Tab1:AddToggle("autoparry", {
    Title = "Auto Parry",
    Default = false,
    Callback = function(value) 
        System.__properties.__autoparry_enabled = value
        if value then
            System.autoparry.start()
        else
            System.autoparry.stop()
        end
    end
})

Tab1:AddToggle("cooldownprotection", {
    Title = "Cooldown Protection",
    Default = false,
    Callback = function(state) 
        getgenv().CooldownProtection = value
    end
})

Tab1:AddSlider("parryaccuracy", {
    Title = "Parry Accuracy",
    Min = 0, 
    Max = 100, 
    Default = 50, 
    Rounding = 0,
    Callback = function(value) 
        System.__properties.__accuracy = value
        update_divisor()
    end
})

Tab1:AddDropdown("autocurve", {
    Title = "Auto Curve Type",
    Values = System.__config.__curve_names,
    Multi = false,
    Default = 1,
    Search = true,
    Callback = function(value)
        for i, name in ipairs(System.__config.__curve_names) do
            if name == value then
                System.__properties.__curve_mode = i
                break
            end
        end
    end
})

Tab2:AddToggle("autospam", {
    Title = "Auto Spam",
    Default = false,
    Callback = function(value) 
        System.__properties.__auto_spam_enabled = value
        if value then
            System.auto_spam.start()
        else
            System.auto_spam.stop()
        end
    end
})

Tab2:AddToggle("manualspam", {
    Title = "Manual Spam",
    Description = "PC: 'E' To Spam",
    Default = false,
    Callback = function(state)
	local props = System.__properties
        props.__mobile_guis = props.__mobile_guis or {}
        props.__connections = props.__connections or {}

        local function cleanup()
            System.manual_spam.stop()

            if props.__connections.manual_spam_keys then
                for _,c in pairs(props.__connections.manual_spam_keys) do
                    c:Disconnect()
                end
                props.__connections.manual_spam_keys = nil
            end

            if props.__mobile_guis.manual_spam then
                destroy_mobile_gui(props.__mobile_guis.manual_spam)
                props.__mobile_guis.manual_spam = nil
            end
        end

        if not state then
            cleanup()
            return
        end

        local TweenService = game:GetService("TweenService")

local btn = create_mobile_button("Spam",0.8,Color3.fromRGB(255,80,80))
props.__mobile_guis.manual_spam = btn
props.__connections.manual_spam_keys = {}

local tweenInfo = TweenInfo.new(
    1,
    Enum.EasingStyle.Quad,
    Enum.EasingDirection.Out
)

local function setTextColor(on)
    if on then
        -- xanh
        TweenService:Create(btn.text, tweenInfo, {
            TextColor3 = Color3.fromRGB(60,255,120)
        }):Play()
    else
        -- 캠敲
        TweenService:Create(btn.text, tweenInfo, {
            TextColor3 = Color3.fromRGB(255,80,80)
        }):Play()
    end
end

local function toggleManualSpam()
    if props.__manual_spam_enabled then
        System.manual_spam.stop()
        props.__manual_spam_enabled = false
        btn.text.Text = "Spam"
        setTextColor(false)
    else
        System.manual_spam.start()
        props.__manual_spam_enabled = true
        btn.text.Text = "Spam"
        setTextColor(true)
    end
end

        table.insert(props.__connections.manual_spam_keys,
            btn.button.MouseButton1Click:Connect(toggleManualSpam)
        )

        table.insert(props.__connections.manual_spam_keys,
            UserInputService.InputBegan:Connect(function(input,gpe)
                if gpe then return end
                if input.KeyCode == Enum.KeyCode.E then
                    toggleManualSpam()
                end
            end)
        )
    end
})

Tab3:AddToggle("detection1", {
    Title = "Global Infinity Detection",
    Default = false,
    Callback = function(value)
		System.__config.__detections.__infinity = value
    end
})

Tab3:AddToggle("detection2", {
    Title = "Global Death Slash Detection",
    Default = false,
    Callback = function(value)
		System.__config.__detections.__deathslash = value
    end
})
